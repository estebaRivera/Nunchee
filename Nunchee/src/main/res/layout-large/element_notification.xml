<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#00000000"
    android:layout_width="match_parent"
    android:layout_height="44dp">

    <TextView
        android:id="@+id/txt_nombre_notification"
        android:text="Programa"
        android:textSize="13sp"
        android:textColor="#FFFFFF"
        android:layout_marginTop="4dp"
        android:layout_marginLeft="47dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/txt_fecha_notification"
        android:text="12:00 -10-10-2019"
        android:textSize="11sp"
        android:textColor="#FFFFFF"
        android:layout_marginTop="22dp"
        android:layout_marginLeft="64dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/img_notifications_icon"
        android:background="@drawable/timer_icon"
        android:layout_marginTop="23dp"
        android:layout_marginLeft="47dp"
        android:layout_width="11dp"
        android:layout_height="11dp"/>

    <ImageView
        android:id="@+id/img_notifications_back"
        android:background="#FFFFFF"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="5dp"
        android:layout_width="32dp"
        android:layout_height="32dp"/>


    <ImageView
        android:id="@+id/img_notifications"
        android:background="@drawable/feed_small_placeholder"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="6dp"
        android:layout_width="30dp"
        android:layout_height="30dp"/>

</FrameLayout>